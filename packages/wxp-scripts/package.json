{
    "name": "@adobe/ccweb-add-on-scripts",
    "version": "0.1.0",
    "author": "Adobe",
    "license": "MIT",
    "description": "Scripts for Adobe Creative Cloud Web Add-on.",
    "type": "module",
    "bin": {
        "ccweb-add-on-scripts": "./bin/run.js"
    },
    "repository": {
        "url": "https://github.com/adobe/create-ccweb-add-on"
    },
    "scripts": {
        "clean": "rimraf dist coverage",
        "ibuild": "tsc",
        "build": "rushx clean && rushx ibuild",
        "build:release": "rushx build",
        "test": "c8 mocha && c8 check-coverage --lines 100 --functions 100 --branches 100"
    },
    "oclif": {
        "bin": "ccweb-add-on-scripts",
        "commands": "./dist/commands",
        "plugins": [],
        "topicSeparator": " "
    },
    "files": [
        "*"
    ],
    "dependencies": {
        "@adobe/ccweb-add-on-analytics": "workspace:*",
        "@adobe/ccweb-add-on-core": "workspace:*",
        "@adobe/ccweb-add-on-devcert": "0.1.0",
        "@adobe/ccweb-add-on-developer-terms": "workspace:*",
        "@adobe/ccweb-add-on-manifest": "workspace:*",
        "@adobe/ccweb-add-on-ssl": "workspace:*",
        "@oclif/core": "2.8.0",
        "adm-zip": "0.5.9",
        "chokidar": "3.5.3",
        "cors": "2.8.5",
        "express": "4.18.2",
        "fs-extra": "10.0.1",
        "inversify": "6.0.1",
        "is-valid-domain": "0.1.6",
        "reflect-metadata": "0.1.13",
        "string-template": "1.0.0",
        "tslib": "2.4.0",
        "ws": "8.9.0"
    },
    "devDependencies": {
        "@oclif/test": "2.3.4",
        "@types/adm-zip": "0.4.34",
        "@types/chai-as-promised": "7.1.3",
        "@types/chai": "4.2.14",
        "@types/cors": "2.8.12",
        "@types/express": "4.17.9",
        "@types/fs-extra": "9.0.13",
        "@types/mocha": "9.1.1",
        "@types/node": "16.11.62",
        "@types/sinon": "9.0.8",
        "@types/string-template": "1.0.2",
        "@types/ws": "8.5.3",
        "c8": "7.7.2",
        "chai-as-promised": "7.1.1",
        "chai": "4.3.4",
        "mocha": "10.0.0",
        "oclif": "3.2.28",
        "prettier": "2.8.0",
        "regenerator-runtime": "0.13.9",
        "rimraf": "3.0.2",
        "sinon": "9.2.1",
        "ts-node": "10.9.1",
        "ts-sinon": "2.0.1",
        "typescript": "4.8.4"
    }
}
