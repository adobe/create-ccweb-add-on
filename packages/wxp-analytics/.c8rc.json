{
    "all": true,
    "include": ["src/**/*.ts"],
    "exclude": [
        "**/*.d.ts",
        "src/**/*.spec.ts",
        "src/**/*Types.ts",
        "src/**/index.ts",
        "src/base/BaseCommand.ts",
        "src/config/*",
        "src/constants.ts"
    ],
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "statements": 100,
    "reporter": ["html", "text", "text-summary"],
    "report-dir": "coverage"
}
