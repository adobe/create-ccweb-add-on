        "@adobe/ccweb-add-on-manifest": "workspace:*",
        "@adobe/ccweb-add-on-ssl": "workspace:*",
        "chalk": "4.1.1",
        "fs-extra": "10.0.1",
        "inversify": "6.0.1",
        "prompts": "2.4.2",
        "reflect-metadata": "0.1.13",
        "string-template": "1.0.0",
        "tslib": "2.4.0",
        "uuid": "8.3.1"
    },
    "devDependencies": {
        "@types/chai-as-promised": "7.1.3",
        "@types/chai": "4.2.14",
        "@types/fs-extra": "9.0.13",
        "@types/mocha": "9.1.1",
        "@types/node": "16.11.62",
        "@types/prompts": "2.0.14",
        "@types/sinon": "9.0.8",
        "@types/string-template": "1.0.2",
        "@types/uuid": "8.3.0",
        "c8": "7.7.2",
        "chai-as-promised": "7.1.1",
        "chai": "4.3.4",
        "mocha": "10.0.0",
        "prettier": "2.8.0",
        "regenerator-runtime": "0.13.9",
        "rimraf": "3.0.2",
        "sinon": "9.2.1",
        "ts-node": "10.9.1",
        "ts-sinon": "2.0.1",
        "typescript": "4.8.4"
    }
