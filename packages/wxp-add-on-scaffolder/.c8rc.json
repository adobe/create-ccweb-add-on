{
    "all": true,
    "include": ["src/**/*.ts"],
    "exclude": ["src/**/*.spec.ts", "src/**/index.ts", "src/config/*", "src/constants.ts"],
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "statements": 100,
    "reporter": ["html", "text", "text-summary"],
    "report-dir": "coverage"
}
